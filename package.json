{
  "name": "test-backend-nodejs",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test-unit": "npm test -- --watch -c jest-unit.config.js",
    "test-integration": "npm test -- --watch -c jest-integration.config.js",
    "dev": "ts-node-dev -r tsconfig-paths/register --files --transpile-only --respawn --ignore-watch node-modules src/server.ts"
  },
  "repository": "https://github.com/vmotta8/test-backend-nodejs.git",
  "author": "Vinicius Motta <viniciusmotta8@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "git-commit-msg-linter": "^3.0.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
